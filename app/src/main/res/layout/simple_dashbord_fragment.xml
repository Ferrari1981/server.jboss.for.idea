<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativelayout_dashboard"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#C0C0C0"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".Dashboard.MainActivity_Dashboard">
    <com.google.android.material.card.MaterialCardView
        android:id="@+id/materialcardview_dashboard"
        android:layout_width="200dp"
        android:layout_height="150dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="50dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/dialog_bg"
        android:animateLayoutChanges="true"
        android:gravity="center"
        app:cardBackgroundColor="#F2F8F8"
        app:cardCornerRadius="15dp"
        app:cardElevation="2dp"
        app:cardMaxElevation="10dp"
        android:translationZ="10dp"
        app:strokeColor="#FCFAFA"
        app:strokeWidth="1dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="visible">


            <ImageView
                android:id="@+id/textview_newtabel"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_centerInParent="true"
                android:translationZ="10dp"
                android:visibility="visible"
                app:cardElevation="1dp"
                app:cardMaxElevation="10dp"
                app:srcCompat="@mipmap/icon_dashdornd_tabel2" />








        </RelativeLayout>
    </com.google.android.material.card.MaterialCardView>
</RelativeLayout>
