<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativelayout_dashboard"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#7CCAED"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".Dashboard.MainActivity_Dashboard">


    <com.google.android.material.card.MaterialCardView
        android:id="@+id/materialcardview_dashboard"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="5dp"
        android:background="@drawable/dialog_bg"
        android:animateLayoutChanges="true"
        android:layout_centerInParent="true"
        android:gravity="center"
        app:cardBackgroundColor="#7CCAED"
        app:cardCornerRadius="1dp"
        app:cardElevation="2dp"
        app:cardMaxElevation="10dp"
        android:translationZ="10dp"
        app:strokeColor="#FCFAFA"
        app:strokeWidth="1dp">


        <RelativeLayout
            android:id="@+id/relativelayout_container_dashboard"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="visible">

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/materialcardview_dashboard_inside1"
                android:layout_width="200dp"
                android:layout_height="150dp"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="50dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/dialog_bg"
                android:animateLayoutChanges="true"
                android:gravity="center"
                app:cardBackgroundColor="#F2F8F8"
                app:cardCornerRadius="15dp"
                app:cardElevation="2dp"
                app:cardMaxElevation="10dp"
                android:translationZ="10dp"
                app:strokeColor="#FCFAFA"
                app:strokeWidth="1dp">

            </com.google.android.material.card.MaterialCardView>


        </RelativeLayout>










    </com.google.android.material.card.MaterialCardView>



















</RelativeLayout>
